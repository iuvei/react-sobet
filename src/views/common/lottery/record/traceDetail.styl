.trace-detail-wrapper {
    .back-bet-modal {
        font-size: 13px;
        font-weight: 400;
        text-decoration: underline;
        color: #f60;
        margin-left: 10px;
        cursor: pointer;
    }

    table {
        border-collapse: collapse;
        border-spacing: 0;

        th, td {
            border: 1px solid #ededed;
            position: relative;
            line-height: 30px;
            padding-left: 10px;
            vertical-align: top;
            white-space: nowrap;
        }

        .fixtrace {
            width: 385px;
            border-left: 2px solid #aaa;
            padding-left: 0;
        }

        ul.traceInner {
            min-height: 413px;
        }

        ul.traceInner li.traceHead {
            border-top: 0;
            background: #ededed;
        }

        ul.traceInner li {
            vertical-align: top;
            margin: 0;
            padding: 0;
            display: block;
            height: 30px;
            border-bottom: #d7d7d7 1px solid;
        }

        ul.traceInner li > span, ul.traceInner li > em {
            display: block;
            float: left;
            text-align: center;
            width: 155px;
            text-indent: 0;
        }

        ul.traceInner li input {
            float: left;
            cursor: pointer;
            margin-top: 9px;
            margin-left: 15px;
        }

        ul.traceInner li em {
            width: 75px;
            border-left: 1px solid #d7d7d7;
        }

        div.popdetails-page {
            height: 45px;
            display: block;
        }

        .popdetails-page {
            padding: 10px 0;
            text-align: center;
            display: none;
        }

        div.cancelTrace a.disabled {
            background-color: #ccc;
            color: #999;
            cursor: default;
        }

        div.cancelTrace a {
            background: #ff5243;
            width: 100px;
            height: 32px;
            font-size: 16px;
            float: right;
            line-height: 32px;
            cursor: pointer;
            color: #fff;
            text-align: center;
            text-indent: 0;
        }

        .traceHead .checkbox-wrapper {
            left: -40px;
        }

        .checkbox-wrapper {
            position: relative;
            bottom: 2px;
            left: -10px;
        }
    }
}