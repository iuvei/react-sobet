    .head-center {
        width: 320px;
        position: relative;
        top: 50%;
        transform: translateY(-50%);

        .head-center-issue, .head-center-clock {
            height: 46px;
        }

        .head-center-clock {
            line-height: 46px;
            font-size: 0;
            background-color: #fff;
            color: #444;
            border-radius: 4px;
            box-shadow: inset 0 0 4px 0 rgba(82, 82, 82, 0.4);

            .wait-to-sell {
                font-size: 26px;
                padding: 0 10px;
                color: #696969;
            }

            .issue-countdown {
                padding: 0 10px;
            }

            .count-down-hours, .count-down-minutes, .count-down-seconds {
                display: inline-block;
                width: 42px;
                height: 46px;
                line-height: 46px;
                text-align: center;
                font-size: 34px;
                font-weight: 700;
            }

            .count-down-symbol {
